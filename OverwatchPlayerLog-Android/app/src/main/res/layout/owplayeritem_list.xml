<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/owplayeritem_list_swipeRefreshLayout"
    android:name="me.benh.overwatchplayerlog.OwPlayerItemListFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/owplayeritem_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layoutManager="LinearLayoutManager"
        tools:context="me.benh.overwatchplayerlog.controllers.listanddetails.OwPlayerItemListActivity"
        tools:listitem="@layout/owplayeritem_list_content" />

</android.support.v4.widget.SwipeRefreshLayout>
